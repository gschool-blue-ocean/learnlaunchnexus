{
  "name": "full-stack-react",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "deploy": "concurrently  \"npm --prefix ./api run start\"  \"npm --prefix ./client run build && npm --prefix ./client run dev\"",
    "lint": "prettier --check .",
    "test": "concurrently \"npm:test:*\"",
    "ci": "npm run lint && npm run test",
    "test:client": "npm run test --prefix=client",
    "test:api": "npm run test --prefix=api"
  },
  "devDependencies": {
    "concurrently": "^7.1.0",
    "prettier": "^2.8.4",
    "nodemon": "^2.0.15",
    "supertest": "^6.3.3",
    "vitest": "^0.29.2",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "jsdom": "^21.1.0",
    "msw": "^1.1.0",
    "vite": "^3.0.9"
  },
  "dependencies": {
    "bcrypt": "^5.1.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.1",
    "pg": "^8.9.0",
    "axios": "^0.27.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.15.0"
  }
}
  